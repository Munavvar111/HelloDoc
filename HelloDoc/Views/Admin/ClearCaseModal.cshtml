<!-- Modal Assign Request --><div class="modal fade" id="ClearModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">    <div class="modal-dialog modal-dialog-centered">        <div class="modal-content">            <div class="modal-header border-0">            </div>            <form id="clearcasedata" asp-action="ClearCase" asp-controller="Admin" method="post">                <input class="d-none" type="hidden" id="ClearCaseModal" name="requestidclearcase"/>                <div class="modal-body">                    <div>                        <div class="text-center mb-3 ">                            <img src="~/Images/warning-ico.png">                        </div>                        <h4 class="text-center">Confirmation for clear case</h4>                        <p class="text-center mx-3">                            Are sure you want to clear this request? Once clear this request                            then you are                            not able to see this request.                        </p>                    </div>                </div>                <div class="modal-footer d-flex justify-content-center border-0">                    <button type="submit" class="btn bg-info text-white" data-bs-dismiss="modal" id="clearcase">Clear</button>                    <button type="reset" class="btn border-info text-info" data-bs-dismiss="modal">Cancel</button>                </div>            </form>        </div>    </div></div>