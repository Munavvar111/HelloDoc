@model List<DataAccessLayer.CustomModel.SearchRecordVM><div class="table-responsive mt-4 mb-4">	<table class="table table-hover">		<thead class="table-light">			<tr>				<th scope="col">Patient Name</th>				<th scope="col">Requester</th>				<th scope="col">Date Of Service</th>				<th scope="col">Email</th>				<th scope="col">Phone Number</th>				<th scope="col">Address</th>				<th scope="col">Zip</th>				<th scope="col">Request Status</th>				<th scope="col">Physician</th>				<th scope="col">Physician Note</th>				<th scope="col">Cancelled By Providor Note</th>				<th scope="col">Admin Note</th>				<th scope="col">Patient Note</th>				<th scope="col">Delete Permanently</th>			</tr>		</thead>		<tbody>			@foreach (var item in Model)			{				<tr>					<td scope="row">						@item.PatientName					</td>					<td scope="row">						@if (item.RequestTypeId == 1)
						{							<span>Business</span>						}						@if (item.RequestTypeId == 2)
						{							<span>Patient</span>						}						@if (item.RequestTypeId == 3)
						{							<span>Family/Friend</span>						}						@if (item.RequestTypeId == 4)
						{							<span>Concierge</span>						}					</td>					<td scope="row">@item.DateOfService</td>					<td scope="row">@item.Email</td>					<td scope="row">@item.PhoneNumber</td>					<td scope="row">@item.Address</td>					<td scope="row">@item.Zip</td>					<td scope="row">@item.RequestStatus</td>					<td scope="row">@item.PhysicianName</td>					<td scope="row">@item.PhysicianNote</td>					<td scope="row">@item.CancelledByProvidor</td>					<td scope="row">@item.AdminNotes</td>					<td scope="row">@item.PatientNote</td>					<td scope="row">						<div class="d-flex">							<a class="btn btn-outline-info rounded me-2">Delete</a>						</div>					</td>				</tr>			}		</tbody>	</table></div> 