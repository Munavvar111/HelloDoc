@model List<DataAccessLayer.CustomModel.NewRequestTableVM>

@{
    Layout = "~/Views/Shared/_Layout4.cshtml";
}

@section scripts{
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    
}
<div class="container-fluid mt-5">
    <div class="row row-cols-3 row-cols-lg-6 g-2 g-lg-3">
        <a class="col" id="linkNew">
            <div class="border text-white p-3" style="background-color:#203f9a">
                <div class="d-flex">
                    <span class="material-symbols-outlined d-none d-md-block">
                        person
                    </span>
                    <span>New</span>
                </div>
                <div>1452</div>
            </div>
        </a>
        <a class="col" id="linkPanding">
            <div class="border p-3 text-info" style="background-color:#e5f2f8">
                <div class="d-flex">
                    <span class="material-symbols-outlined d-none d-md-block">
                        group
                    </span>
                    <span>Pending</span>
                </div>
                <div class="fw-bold">256</div>
            </div>
        </a>
        <a class="col" id="linkActive">
            <div class="border p-3" style="background-color:#e9f0e8">
                <div class="d-flex">
                    <span class="material-symbols-outlined d-none d-md-block">
                        check_circle
                    </span>
            <span>Active</span>
                </div>
            <div>1452</div>
            </div>

        </a>
        <a class="col" id="linkConclude">
            <div class="border p-3 text-pink" style="background-color:#f7e6ee">
                <div class="d-flex">
                    <span class="material-symbols-outlined d-none d-md-block">
                        history
                    </span>
            <span>Conclude</span>
                </div>
            <div class="fw-blod">1452</div>
            </div>

        </a>
        <a class="col" id="linkToclose">
            <div class="border p-3" style="background-color:#e5eef5">
                <div class="d-flex">
                    <span class="material-symbols-outlined d-none d-md-block">
                        cancel
                    </span>
            <span >To Close</span>
                </div>
            <div class="fw-blod">1452</div>
            </div>

        </a>
        <a class="col" id="linkUnpaid"> 
            <div class="border p-3" style="background-color: #efeef4">
                <div class="d-flex ">
                    <span class="material-symbols-outlined d-none d-md-block">
                        currency_exchange
                    </span>
            <span>Unpaid</span>
                </div>

            <div class="fw-blod">1452</div>
            </div>

        </a>

    </div>
</div>
<div class="mt-3 mt-md-5 d-flex-column d-sm-flex justify-content-between align-items-center">
    <div class="">
        <p class="patient_text_size ">Patients<span class="text-primary">(new)</span></p>
    </div>
    <div class="">
        <button type="button" class="btn btn-info text-white">
            <i class="bi bi-send  me-md-2"></i><span class="d-none d-lg-inline">Send Link</span>
        </button>
        <button type="button" class="btn btn-info  text-white">
            <i class="bi bi-pencil-square me-md-2"></i><span class="d-none d-lg-inline">
                Create
                Requests
            </span>
        </button>
        <button type="button" class="btn btn-info  text-white">
            <i class="bi bi-arrow-up-right "></i>
            <span class="d-none d-lg-inline ">Export</span>
        </button>
        <button type="button" class="btn btn-info  text-white">
            <i class="bi bi-box-arrow-up-right "></i><span class="d-none d-lg-inline">
                Export
                All
            </span>
        </button>
        <button type="button" class="btn btn-info  text-white">
            <i class="bi bi-person-plus  me-md-2"></i><span class="d-none d-lg-inline">
                Request DTY
                Support
            </span>
        </button>
    </div>
</div>
<div class="bg-white row mt-4 container-fluid">
    <div class="col-xl-2 col-md-4 col-sm-6">
        <div class="input-group rounded">
            
            <input type="text" class="form-control" placeholder="Search Patients" id="searchInput" />
            
        </div>
    </div>

    <div class="col-xl-2 col-md-4 col-sm-6">
        <div class="input-group rounded">
           
            <select class="form-select" aria-label="Default select example" id="filterSelect">
                <option selected value="">Open this select menu</option>
                <option value="1">One</option>
                <option value="2">Two</option>
                <option value="3">Three</option>
            </select>
           
        </div>
    </div>

    <div class="col-md-0 col-lg-1 col-xxl-3">

    </div>
    <div class="col-1">
        All
    </div>
     <div class="col-1">
        Patient
    </div>
     <div class="col-1">
        family/friend
    </div>
     <div class="col-1">
        Business
    </div>
     <div class="col-1">
        Conciege
    </div>


</div>

<div>

</div>
<div id="partialContainer">

   
    <partial name="NewTablePartial" model="@Model"></partial>
    <partial name="PendingTablePartial" model="@Model"></partial>
    <partial name="ActiveTablePartial" model="@Model"></partial>
    <partial name="ConcludeTablePartial" model="@Model"></partial>
    <partial name="ToCloseTablePartial" model="@Model"></partial>
    <partial name="UnpaidTablePartial" model="@Model"></partial>
</div>
<div>
</div>